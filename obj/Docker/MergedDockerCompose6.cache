name: dockercompose5462879562128122803
services:
  elasticsearch:
    container_name: elasticsearch
    environment:
      discovery.type: single-node
      xpack.security.enabled: "false"
    image: docker.elastic.co/elasticsearch/elasticsearch:7.11.0
    networks:
      nw: null
    ports:
    - mode: ingress
      target: 9200
      published: "9200"
      protocol: tcp
    volumes:
    - type: volume
      source: v01
      target: /usr/share/elasticsearch/data
      volume: {}
  kibana:
    depends_on:
      elasticsearch:
        condition: service_started
    environment:
      ELASTICSEARCH_HOSTS: http://elasticsearch:9200
      ELASTICSEARCH_URL: http://elasticsearch:9200
    image: docker.elastic.co/kibana/kibana:7.11.0
    networks:
      nw: null
    ports:
    - mode: ingress
      target: 5601
      published: "5601"
      protocol: tcp
  logger:
    build:
      context: C:\dev\Logger
      dockerfile: Logger/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: faulycoelho/logger-elasticsearch-kibana
    networks:
      nw: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
networks:
  nw:
    name: dockercompose5462879562128122803_nw
    driver: bridge
volumes:
  v01:
    name: dockercompose5462879562128122803_v01
    driver: local